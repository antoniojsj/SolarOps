{
	"name": "solarops",
	"version": "1.0.0",
	"description": "This plugin template uses Typescript. If you are familiar with Javascript, Typescript will look very familiar. In fact, valid Javascript code is already valid Typescript code.",
	"license": "ISC",
	"scripts": {
		"build": "NODE_OPTIONS='--openssl-legacy-provider' /usr/local/bin/node node_modules/.bin/webpack --mode=production",
		"build:watch": "NODE_OPTIONS='--openssl-legacy-provider' /usr/local/bin/node node_modules/.bin/webpack --mode=development --watch",
		"prettier:format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}' "
	},
	"dependencies": {
		"@types/html2canvas": "^1.0.0",
		"@types/prismjs": "^1.26.5",
		"@types/styled-components": "^5.1.34",
		"classnames": "^2.2.6",
		"framer-motion": "^6.3.0",
		"html2canvas": "^1.4.1",
		"mobx": "^6.13.7",
		"mobx-react-lite": "^4.1.0",
		"prismjs": "^1.30.0",
		"react": "^17.0.2",
		"react-color": "^2.19.3",
		"react-colorful": "^5.6.1",
		"react-dom": "^17.0.2",
		"react-hint": "^3.2.1",
		"react-simple-code-editor": "^0.14.1",
		"react-tooltip": "^5.11.1",
		"styled-components": "^6.1.19"
	},
	"devDependencies": {
		"@figma/plugin-typings": "^1.114.0",
		"@svgr/webpack": "5.5.0",
		"@types/node": "^18.19.0",
		"@types/react": "^16.8.24",
		"@types/react-color": "^3.0.13",
		"@types/react-dom": "^16.8.5",
		"copy-webpack-plugin": "6.4.1",
		"css-loader": "^3.1.0",
		"html-webpack-inline-source-plugin": "^0.0.10",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^3.0.2",
		"lint-staged": "^9.2.1",
		"prettier": "^1.18.2",
		"style-loader": "^0.23.1",
		"ts-loader": "^6.0.4",
		"tslint": "^5.18.0",
		"tslint-react": "^4.0.0",
		"typescript": "^4.9.5",
		"url-loader": "^2.1.0",
		"webpack": "^4.39.1",
		"webpack-cli": "^3.3.6"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,css,json}": [
			"prettier --write",
			"git add"
		]
	}
}
